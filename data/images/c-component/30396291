digraph {
	graph [rankdir=LR]
	D [height=.5 shape=plaintext width=.5]
	Y [height=.5 shape=plaintext width=.5]
	G [height=.5 shape=plaintext width=.5]
	K [height=.5 shape=plaintext width=.5]
	W [height=.5 shape=plaintext width=.5]
	N [height=.5 shape=plaintext width=.5]
	K -> N [color=blue]
	D -> G [color=blue]
	D -> N [color=blue]
	G -> N [color=blue]
	Y -> G [color=blue]
	G -> W [color=blue]
	Y -> W [color=blue]
	Y -> K [color=red dir=both]
}
