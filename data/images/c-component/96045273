digraph {
	graph [rankdir=LR]
	K [height=.5 shape=plaintext width=.5]
	S [height=.5 shape=plaintext width=.5]
	T [height=.5 shape=plaintext width=.5]
	G [height=.5 shape=plaintext width=.5]
	C [height=.5 shape=plaintext width=.5]
	S -> G [color=blue]
	K -> C [color=blue]
	T -> C [color=blue]
	S -> T [color=blue]
	S -> T [color=red dir=both]
	T -> C [color=red dir=both]
	G -> C [color=red dir=both]
	K -> G [color=red dir=both]
}
