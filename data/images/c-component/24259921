digraph {
	graph [rankdir=LR]
	W [height=.5 shape=plaintext width=.5]
	E [height=.5 shape=plaintext width=.5]
	Z [height=.5 shape=plaintext width=.5]
	N [height=.5 shape=plaintext width=.5]
	E -> N [color=blue]
	Z -> N [color=blue]
	W -> Z [color=blue]
	W -> N [color=blue]
	E -> Z [color=red dir=both]
	Z -> N [color=red dir=both]
	W -> Z [color=red dir=both]
}
