digraph {
	graph [rankdir=LR]
	M [height=.5 shape=plaintext width=.5]
	T [height=.5 shape=plaintext width=.5]
	Z [height=.5 shape=plaintext width=.5]
	G [height=.5 shape=plaintext width=.5]
	N [height=.5 shape=plaintext width=.5]
	Z -> N [color=blue]
	M -> Z [color=blue]
	T -> G [color=blue]
	Z -> G [color=blue]
	G -> N [color=blue]
	Z -> N [color=red dir=both]
	M -> T [color=red dir=both]
	T -> G [color=red dir=both]
}
