digraph {
	graph [rankdir=LR]
	Y [height=.5 shape=plaintext width=.5]
	V [height=.5 shape=plaintext width=.5]
	S [height=.5 shape=plaintext width=.5]
	O [height=.5 shape=plaintext width=.5]
	T [height=.5 shape=plaintext width=.5]
	Z [height=.5 shape=plaintext width=.5]
	O -> T [color=blue]
	V -> T [color=blue]
	T -> Z [color=blue]
	V -> S [color=blue]
	Y -> Z [color=blue]
	Y -> S [color=blue]
	S -> Z [color=red dir=both]
	Y -> S [color=red dir=both]
	T -> Z [color=red dir=both]
}
