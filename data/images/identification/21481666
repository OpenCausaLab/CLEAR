digraph {
	graph [rankdir=LR]
	A [height=.5 shape=plaintext width=.5]
	Y [height=.5 shape=plaintext width=.5]
	C [height=.5 shape=plaintext width=.5]
	O [height=.5 shape=plaintext width=.5]
	E [height=.5 shape=plaintext width=.5]
	A -> E [color=blue]
	O -> E [color=blue]
	Y -> C [color=blue]
	A -> O [color=blue]
	Y -> E [color=blue]
	C -> E [color=blue]
	A -> Y [color=red dir=both]
	A -> C [color=red dir=both]
}
