digraph {
	graph [rankdir=LR]
	H [height=.5 shape=plaintext width=.5]
	I [height=.5 shape=plaintext width=.5]
	A [height=.5 shape=plaintext width=.5]
	S [height=.5 shape=plaintext width=.5]
	U [height=.5 shape=plaintext width=.5]
	I -> S [color=blue]
	A -> U [color=blue]
	H -> A [color=blue]
	S -> U [color=blue]
	I -> U [color=blue]
	A -> S [color=blue]
	I -> A [color=blue]
	I -> S [color=red dir=both]
	H -> U [color=red dir=both]
	A -> S [color=red dir=both]
	H -> I [color=red dir=both]
}
