digraph {
	graph [rankdir=LR]
	E [height=.5 shape=plaintext width=.5]
	M [height=.5 shape=plaintext width=.5]
	B [height=.5 shape=plaintext width=.5]
	H [height=.5 shape=plaintext width=.5]
	M -> B [color=blue]
	M -> H [color=blue]
	E -> M [color=blue]
	E -> H [color=blue]
	B -> H [color=blue]
	E -> M [color=red dir=both]
}
