digraph {
	graph [rankdir=LR]
	W [height=.5 shape=plaintext width=.5]
	K [height=.5 shape=plaintext width=.5]
	P [height=.5 shape=plaintext width=.5]
	S [height=.5 shape=plaintext width=.5]
	W -> K [color=blue]
	W -> P [color=blue]
	K -> S [color=blue]
	W -> S [color=blue]
	K -> P [color=blue]
	P -> S [color=blue]
	W -> P [color=red dir=both]
	K -> P [color=red dir=both]
	P -> S [color=red dir=both]
}
