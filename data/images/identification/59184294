digraph {
	graph [rankdir=LR]
	V [height=.5 shape=plaintext width=.5]
	F [height=.5 shape=plaintext width=.5]
	B [height=.5 shape=plaintext width=.5]
	Z [height=.5 shape=plaintext width=.5]
	Y [height=.5 shape=plaintext width=.5]
	F -> Y [color=blue]
	B -> Z [color=blue]
	F -> B [color=blue]
	V -> F [color=blue]
	V -> Y [color=blue]
	V -> Z [color=red dir=both]
	B -> Z [color=red dir=both]
	Z -> Y [color=red dir=both]
}
