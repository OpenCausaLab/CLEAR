digraph {
	graph [rankdir=LR]
	Y [height=.5 shape=plaintext width=.5]
	L [height=.5 shape=plaintext width=.5]
	T [height=.5 shape=plaintext width=.5]
	B [height=.5 shape=plaintext width=.5]
	K [height=.5 shape=plaintext width=.5]
	L -> T [color=blue]
	B -> K [color=blue]
	T -> B [color=blue]
	Y -> K [color=blue]
	Y -> T [color=blue]
	Y -> B [color=blue]
	L -> B [color=red dir=both]
	B -> K [color=red dir=both]
}
