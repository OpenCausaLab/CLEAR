digraph {
	graph [rankdir=LR]
	H [height=.5 shape=plaintext width=.5]
	I [height=.5 shape=plaintext width=.5]
	O [height=.5 shape=plaintext width=.5]
	X [height=.5 shape=plaintext width=.5]
	R [height=.5 shape=plaintext width=.5]
	O -> X [color=blue]
	I -> X [color=blue]
	H -> O [color=blue]
	O -> R [color=blue]
	X -> R [color=blue]
	I -> O [color=blue]
	H -> X [color=blue]
	O -> R [color=red dir=both]
	H -> R [color=red dir=both]
	H -> X [color=red dir=both]
	X -> R [color=red dir=both]
}
