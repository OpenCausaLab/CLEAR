digraph {
	graph [rankdir=LR]
	I [height=.5 shape=plaintext width=.5]
	V [height=.5 shape=plaintext width=.5]
	S [height=.5 shape=plaintext width=.5]
	D [height=.5 shape=plaintext width=.5]
	Y [height=.5 shape=plaintext width=.5]
	V -> S [color=blue]
	I -> V [color=blue]
	D -> Y [color=blue]
	V -> Y [color=blue]
	S -> D [color=blue]
	I -> S [color=blue]
	I -> D [color=blue]
	S -> Y [color=blue]
	I -> Y [color=blue]
}
