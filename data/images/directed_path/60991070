digraph {
	graph [rankdir=LR]
	U [height=.5 shape=plaintext width=.5]
	A [height=.5 shape=plaintext width=.5]
	Q [height=.5 shape=plaintext width=.5]
	F [height=.5 shape=plaintext width=.5]
	N [height=.5 shape=plaintext width=.5]
	A -> N [color=blue]
	U -> A [color=blue]
	U -> N [color=blue]
	Q -> F [color=blue]
	Q -> N [color=blue]
	U -> F [color=blue]
	F -> N [color=blue]
	A -> N [color=red dir=both]
	A -> Q [color=red dir=both]
	Q -> N [color=red dir=both]
}
