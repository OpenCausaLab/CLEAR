digraph {
	graph [rankdir=LR]
	C [height=.5 shape=plaintext width=.5]
	F [height=.5 shape=plaintext width=.5]
	S [height=.5 shape=plaintext width=.5]
	T [height=.5 shape=plaintext width=.5]
	U [height=.5 shape=plaintext width=.5]
	C -> T [color=blue]
	T -> U [color=blue]
	F -> U [color=blue]
	C -> U [color=blue]
	F -> S [color=blue]
	F -> T [color=blue]
	C -> F [color=blue]
}
