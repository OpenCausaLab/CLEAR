digraph {
	graph [rankdir=LR]
	K [height=.5 shape=plaintext width=.5]
	Q [height=.5 shape=plaintext width=.5]
	I [height=.5 shape=plaintext width=.5]
	L [height=.5 shape=plaintext width=.5]
	D [height=.5 shape=plaintext width=.5]
	I -> L [color=blue]
	K -> L [color=blue]
	Q -> L [color=blue]
	I -> D [color=blue]
	K -> D [color=blue]
	Q -> I [color=blue]
	Q -> D [color=blue]
	I -> L [color=red dir=both]
	Q -> I [color=red dir=both]
	K -> Q [color=red dir=both]
}
