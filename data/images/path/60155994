digraph {
	graph [rankdir=LR]
	I [height=.5 shape=plaintext width=.5]
	N [height=.5 shape=plaintext width=.5]
	W [height=.5 shape=plaintext width=.5]
	E [height=.5 shape=plaintext width=.5]
	B [height=.5 shape=plaintext width=.5]
	I -> N [color=blue]
	I -> E [color=blue]
	I -> B [color=blue]
	N -> W [color=blue]
	N -> B [color=blue]
	W -> B [color=blue]
	N -> E [color=blue]
	W -> E [color=blue]
	I -> E [color=red dir=both]
}
