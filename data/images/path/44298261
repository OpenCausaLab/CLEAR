digraph {
	graph [rankdir=LR]
	Q [height=.5 shape=plaintext width=.5]
	H [height=.5 shape=plaintext width=.5]
	I [height=.5 shape=plaintext width=.5]
	D [height=.5 shape=plaintext width=.5]
	N [height=.5 shape=plaintext width=.5]
	G [height=.5 shape=plaintext width=.5]
	Q -> H [color=blue]
	H -> D [color=blue]
	I -> N [color=blue]
	D -> N [color=blue]
	Q -> G [color=blue]
	Q -> D [color=blue]
	H -> I [color=red dir=both]
	D -> G [color=red dir=both]
}
