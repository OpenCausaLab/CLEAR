digraph {
	graph [rankdir=LR]
	Y [height=.5 shape=plaintext width=.5]
	S [height=.5 shape=plaintext width=.5]
	T [height=.5 shape=plaintext width=.5]
	K [height=.5 shape=plaintext width=.5]
	C [height=.5 shape=plaintext width=.5]
	S -> T [color=blue]
	Y -> K [color=blue]
	T -> C [color=blue]
	Y -> S [color=blue]
}
