digraph {
	graph [rankdir=LR]
	Q [height=.5 shape=plaintext width=.5]
	H [height=.5 shape=plaintext width=.5]
	G [height=.5 shape=plaintext width=.5]
	Y [height=.5 shape=plaintext width=.5]
	L [height=.5 shape=plaintext width=.5]
	T [height=.5 shape=plaintext width=.5]
	Q -> Y [color=blue]
	H -> Y [color=blue]
	Y -> T [color=blue]
	H -> T [color=blue]
	Q -> G [color=blue]
	G -> L [color=blue]
	G -> Y [color=blue]
	Y -> L [color=blue]
	H -> L [color=blue]
	Q -> H [color=blue]
}
