digraph {
	graph [rankdir=LR]
	I [height=.5 shape=plaintext width=.5]
	Y [height=.5 shape=plaintext width=.5]
	B [height=.5 shape=plaintext width=.5]
	R [height=.5 shape=plaintext width=.5]
	X [height=.5 shape=plaintext width=.5]
	I -> R [color=blue]
	B -> R [color=blue]
	Y -> R [color=blue]
	I -> X [color=blue]
	I -> Y [color=blue]
	Y -> X [color=blue]
	B -> X [color=blue]
	I -> B [color=blue]
	Y -> B [color=blue]
	R -> X [color=blue]
}
