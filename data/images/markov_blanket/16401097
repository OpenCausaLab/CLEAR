digraph {
	graph [rankdir=LR]
	Q [height=.5 shape=plaintext width=.5]
	N [height=.5 shape=plaintext width=.5]
	C [height=.5 shape=plaintext width=.5]
	B [height=.5 shape=plaintext width=.5]
	T [height=.5 shape=plaintext width=.5]
	F [height=.5 shape=plaintext width=.5]
	R [height=.5 shape=plaintext width=.5]
	N -> C [color=blue]
	F -> R [color=blue]
	Q -> R [color=blue]
	B -> F [color=blue]
	Q -> T [color=blue]
	C -> T [color=blue]
}
