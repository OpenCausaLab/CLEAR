digraph {
	graph [rankdir=LR]
	N [height=.5 shape=plaintext width=.5]
	Z [height=.5 shape=plaintext width=.5]
	H [height=.5 shape=plaintext width=.5]
	L [height=.5 shape=plaintext width=.5]
	U [height=.5 shape=plaintext width=.5]
	O [height=.5 shape=plaintext width=.5]
	N -> U [color=blue]
	L -> O [color=blue]
	N -> O [color=blue]
	U -> O [color=blue]
	N -> Z [color=blue]
	Z -> H [color=blue]
	N -> L [color=blue]
	L -> U [color=blue]
	H -> O [color=blue]
}
