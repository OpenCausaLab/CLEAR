digraph {
	graph [rankdir=LR]
	F [height=.5 shape=plaintext width=.5]
	H [height=.5 shape=plaintext width=.5]
	A [height=.5 shape=plaintext width=.5]
	D [height=.5 shape=plaintext width=.5]
	C [height=.5 shape=plaintext width=.5]
	S [height=.5 shape=plaintext width=.5]
	H -> D [color=blue]
	F -> D [color=blue]
	H -> A [color=blue]
	A -> C [color=blue]
	F -> H [color=blue]
	D -> S [color=blue]
	A -> S [color=blue]
	H -> S [color=blue]
	D -> C [color=blue]
}
