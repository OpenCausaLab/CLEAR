digraph {
	graph [rankdir=LR]
	P [height=.5 shape=plaintext width=.5]
	K [height=.5 shape=plaintext width=.5]
	N [height=.5 shape=plaintext width=.5]
	J [height=.5 shape=plaintext width=.5]
	S [height=.5 shape=plaintext width=.5]
	H [height=.5 shape=plaintext width=.5]
	G [height=.5 shape=plaintext width=.5]
	N -> H [color=blue]
	J -> S [color=blue]
	N -> J [color=blue]
	P -> N [color=blue]
	N -> G [color=blue]
	K -> S [color=blue]
	H -> G [color=blue]
}
