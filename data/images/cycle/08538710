digraph {
	graph [rankdir=LR]
	S [height=.5 shape=plaintext width=.5]
	T [height=.5 shape=plaintext width=.5]
	X [height=.5 shape=plaintext width=.5]
	Q [height=.5 shape=plaintext width=.5]
	M [height=.5 shape=plaintext width=.5]
	N [height=.5 shape=plaintext width=.5]
	U [height=.5 shape=plaintext width=.5]
	M -> T [color=blue]
	T -> U [color=blue]
	S -> N [color=blue]
	U -> M [color=blue]
	T -> X [color=blue]
	X -> M [color=blue]
	M -> Q [color=blue]
	S -> T [color=blue]
}
