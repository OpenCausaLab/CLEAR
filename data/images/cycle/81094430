	graph [rankdir=LR]
	C [height=.5 shape=plaintext width=.5]
	E [height=.5 shape=plaintext width=.5]
	H [height=.5 shape=plaintext width=.5]
	F [height=.5 shape=plaintext width=.5]
	B [height=.5 shape=plaintext width=.5]
	D [height=.5 shape=plaintext width=.5]
	F -> D [color=blue]
	F -> E [color=blue]
	B -> C [color=blue]
	E -> B [color=blue]
	D -> B [color=blue]
	C -> F [color=blue]
	D -> F [color=blue]
	B -> E [color=blue]
	H -> B [color=blue]
	F -> C [color=blue]
}
