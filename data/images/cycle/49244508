digraph {
	graph [rankdir=LR]
	H [height=.5 shape=plaintext width=.5]
	O [height=.5 shape=plaintext width=.5]
	B [height=.5 shape=plaintext width=.5]
	I [height=.5 shape=plaintext width=.5]
	C [height=.5 shape=plaintext width=.5]
	Z [height=.5 shape=plaintext width=.5]
	I -> O [color=blue]
	C -> B [color=blue]
	I -> H [color=blue]
	H -> C [color=blue]
	Z -> I [color=blue]
	C -> O [color=blue]
	C -> H [color=blue]
	C -> I [color=blue]
	O -> H [color=blue]
	Z -> O [color=blue]
}
