digraph {
	graph [rankdir=LR]
	E [height=.5 shape=plaintext width=.5]
	G [height=.5 shape=plaintext width=.5]
	U [height=.5 shape=plaintext width=.5]
	V [height=.5 shape=plaintext width=.5]
	S [height=.5 shape=plaintext width=.5]
	S -> G [color=blue]
	U -> V [color=blue]
	V -> U [color=blue]
	E -> G [color=blue]
	S -> V [color=blue]
	V -> G [color=blue]
	G -> S [color=blue]
	S -> U [color=blue]
	S -> E [color=blue]
}
