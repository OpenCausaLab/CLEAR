digraph {
	graph [rankdir=LR]
	K [height=.5 shape=plaintext width=.5]
	M [height=.5 shape=plaintext width=.5]
	C [height=.5 shape=plaintext width=.5]
	U [height=.5 shape=plaintext width=.5]
	Y [height=.5 shape=plaintext width=.5]
	W [height=.5 shape=plaintext width=.5]
	Y -> K [color=blue]
	U -> C [color=blue]
	K -> U [color=blue]
	Y -> C [color=blue]
	K -> C [color=blue]
	C -> K [color=blue]
	K -> W [color=blue]
	W -> C [color=blue]
	C -> U [color=blue]
	M -> U [color=blue]
}
