digraph {
	graph [rankdir=LR]
	W [height=.5 shape=plaintext width=.5]
	K [height=.5 shape=plaintext width=.5]
	N [height=.5 shape=plaintext width=.5]
	R [height=.5 shape=plaintext width=.5]
	N -> R [color=blue]
	K -> N [color=blue]
	N -> W [color=blue]
	R -> N [color=blue]
	R -> K [color=blue]
	K -> R [color=blue]
	K -> W [color=blue]
	R -> W [color=blue]
}
