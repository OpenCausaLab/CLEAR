digraph {
	graph [rankdir=LR]
	I [height=.5 shape=plaintext width=.5]
	Z [height=.5 shape=plaintext width=.5]
	X [height=.5 shape=plaintext width=.5]
	Y [height=.5 shape=plaintext width=.5]
	M [height=.5 shape=plaintext width=.5]
	W [height=.5 shape=plaintext width=.5]
	Y -> Z [color=blue]
	M -> W [color=blue]
	I -> Z [color=blue]
	Y -> I [color=blue]
	I -> W [color=blue]
	W -> Z [color=blue]
	X -> Z [color=blue]
	Y -> X [color=blue]
	Z -> Y [color=blue]
	I -> X [color=blue]
}
