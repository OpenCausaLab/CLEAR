digraph {
	graph [rankdir=LR]
	T [height=.5 shape=plaintext width=.5]
	A [height=.5 shape=plaintext width=.5]
	X [height=.5 shape=plaintext width=.5]
	E [height=.5 shape=plaintext width=.5]
	C [height=.5 shape=plaintext width=.5]
	A -> E [color=blue]
	T -> E [color=blue]
	T -> X [color=blue]
	E -> C [color=blue]
	E -> A [color=blue]
	X -> C [color=blue]
	E -> X [color=blue]
}
