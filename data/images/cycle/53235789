digraph {
	graph [rankdir=LR]
	U [height=.5 shape=plaintext width=.5]
	Q [height=.5 shape=plaintext width=.5]
	V [height=.5 shape=plaintext width=.5]
	F [height=.5 shape=plaintext width=.5]
	Y [height=.5 shape=plaintext width=.5]
	N [height=.5 shape=plaintext width=.5]
	F -> V [color=blue]
	Q -> F [color=blue]
	U -> Q [color=blue]
	Y -> F [color=blue]
	Q -> N [color=blue]
	F -> U [color=blue]
	Y -> V [color=blue]
	Q -> U [color=blue]
}
