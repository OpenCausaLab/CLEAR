digraph {
	graph [rankdir=LR]
	G [height=.5 shape=plaintext width=.5]
	E [height=.5 shape=plaintext width=.5]
	L [height=.5 shape=plaintext width=.5]
	J [height=.5 shape=plaintext width=.5]
	G -> L [color=blue]
	E -> G [color=blue]
	J -> G [color=blue]
	J -> L [color=blue]
	G -> E [color=blue]
	L -> G [color=blue]
	L -> J [color=blue]
}
