digraph {
	graph [rankdir=LR]
	Z [height=.5 shape=plaintext width=.5]
	T [height=.5 shape=plaintext width=.5]
	P [height=.5 shape=plaintext width=.5]
	S [height=.5 shape=plaintext width=.5]
	T -> P [color=blue]
	Z -> T [color=blue]
	P -> S [color=blue]
	T -> S [color=blue]
	T -> P [color=red dir=both]
	P -> S [color=red dir=both]
}
