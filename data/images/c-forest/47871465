digraph {
	graph [rankdir=LR]
	P [height=.5 shape=plaintext width=.5]
	N [height=.5 shape=plaintext width=.5]
	R [height=.5 shape=plaintext width=.5]
	S [height=.5 shape=plaintext width=.5]
	B [height=.5 shape=plaintext width=.5]
	N -> R [color=blue]
	S -> B [color=blue]
	P -> B [color=blue]
	R -> B [color=blue]
	P -> S [color=blue]
	P -> N [color=blue]
	N -> B [color=blue]
	R -> S [color=blue]
	N -> S [color=blue]
	P -> S [color=red dir=both]
	N -> R [color=red dir=both]
}
