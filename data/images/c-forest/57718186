digraph {
	graph [rankdir=LR]
	N [height=.5 shape=plaintext width=.5]
	O [height=.5 shape=plaintext width=.5]
	X [height=.5 shape=plaintext width=.5]
	A [height=.5 shape=plaintext width=.5]
	S [height=.5 shape=plaintext width=.5]
	X -> S [color=blue]
	A -> S [color=blue]
	N -> A [color=blue]
	O -> A [color=blue]
	X -> A [color=blue]
	O -> X [color=blue]
	O -> S [color=blue]
	O -> A [color=red dir=both]
	X -> S [color=red dir=both]
	N -> S [color=red dir=both]
}
