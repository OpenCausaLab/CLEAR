digraph {
	graph [rankdir=LR]
	Y [height=.5 shape=plaintext width=.5]
	M [height=.5 shape=plaintext width=.5]
	V [height=.5 shape=plaintext width=.5]
	A [height=.5 shape=plaintext width=.5]
	P [height=.5 shape=plaintext width=.5]
	V -> P [color=blue]
	A -> P [color=blue]
	Y -> V [color=blue]
	Y -> P [color=blue]
	M -> A [color=blue]
	Y -> M [color=blue]
	M -> P [color=blue]
	V -> A [color=blue]
	Y -> P [color=red dir=both]
	Y -> M [color=red dir=both]
	Y -> V [color=red dir=both]
}
