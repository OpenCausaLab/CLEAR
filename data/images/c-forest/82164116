digraph {
	graph [rankdir=LR]
	V [height=.5 shape=plaintext width=.5]
	A [height=.5 shape=plaintext width=.5]
	Z [height=.5 shape=plaintext width=.5]
	W [height=.5 shape=plaintext width=.5]
	L [height=.5 shape=plaintext width=.5]
	Z -> W [color=blue]
	A -> W [color=blue]
	V -> Z [color=blue]
	Z -> W [color=red dir=both]
	V -> L [color=red dir=both]
	A -> W [color=red dir=both]
	Z -> L [color=red dir=both]
}
