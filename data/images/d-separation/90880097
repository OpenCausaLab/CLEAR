digraph {
	graph [rankdir=LR]
	F [height=.5 shape=plaintext width=.5]
	D [height=.5 shape=plaintext width=.5]
	K [height=.5 shape=plaintext width=.5]
	Y [height=.5 shape=plaintext width=.5]
	L [height=.5 shape=plaintext width=.5]
	G [height=.5 shape=plaintext width=.5]
	F -> G [color=blue]
	Y -> L [color=blue]
	F -> Y [color=blue]
	L -> G [color=blue]
	Y -> G [color=blue]
	D -> K [color=blue]
	D -> G [color=blue]
	D -> Y [color=blue]
	K -> L [color=blue]
	K -> G [color=blue]
}
