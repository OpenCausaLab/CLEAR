digraph {
	graph [rankdir=LR]
	K [height=.5 shape=plaintext width=.5]
	T [height=.5 shape=plaintext width=.5]
	G [height=.5 shape=plaintext width=.5]
	N [height=.5 shape=plaintext width=.5]
	U [height=.5 shape=plaintext width=.5]
	T -> N [color=blue]
	G -> U [color=blue]
	K -> N [color=blue]
	G -> K [color=blue]
	T -> K [color=blue]
	N -> U [color=blue]
	G -> T [color=blue]
	K -> U [color=blue]
	G -> N [color=blue]
}
