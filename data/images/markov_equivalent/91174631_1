digraph {
	graph [rankdir=LR]
	W [height=.5 shape=plaintext width=.5]
	P [height=.5 shape=plaintext width=.5]
	D [height=.5 shape=plaintext width=.5]
	R [height=.5 shape=plaintext width=.5]
	X [height=.5 shape=plaintext width=.5]
	U [height=.5 shape=plaintext width=.5]
	U -> X [color=blue]
	R -> U [color=blue]
	R -> P [color=blue]
	W -> P [color=blue]
	P -> U [color=blue]
	R -> X [color=blue]
	W -> X [color=blue]
	D -> U [color=blue]
	W -> D [color=blue]
	R -> W [color=blue]
}
