digraph {
	graph [rankdir=LR]
	V [height=.5 shape=plaintext width=.5]
	I [height=.5 shape=plaintext width=.5]
	S [height=.5 shape=plaintext width=.5]
	U [height=.5 shape=plaintext width=.5]
	E [height=.5 shape=plaintext width=.5]
	T [height=.5 shape=plaintext width=.5]
	S -> U [color=blue]
	I -> E [color=blue]
	I -> V [color=blue]
	S -> E [color=blue]
	S -> V [color=blue]
	I -> T [color=blue]
	V -> T [color=blue]
	S -> T [color=blue]
	S -> I [color=blue]
	U -> E [color=blue]
}
