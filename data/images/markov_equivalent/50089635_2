digraph {
	graph [rankdir=LR]
	I [height=.5 shape=plaintext width=.5]
	K [height=.5 shape=plaintext width=.5]
	L [height=.5 shape=plaintext width=.5]
	F [height=.5 shape=plaintext width=.5]
	A [height=.5 shape=plaintext width=.5]
	S [height=.5 shape=plaintext width=.5]
	L -> A [color=blue]
	K -> I [color=blue]
	F -> A [color=blue]
	I -> F [color=blue]
	A -> S [color=blue]
	K -> S [color=blue]
	L -> S [color=blue]
	I -> L [color=blue]
	K -> F [color=blue]
}
