digraph {
	graph [rankdir=LR]
	J [height=.5 shape=plaintext width=.5]
	Z [height=.5 shape=plaintext width=.5]
	Y [height=.5 shape=plaintext width=.5]
	T [height=.5 shape=plaintext width=.5]
	D [height=.5 shape=plaintext width=.5]
	A [height=.5 shape=plaintext width=.5]
	K [height=.5 shape=plaintext width=.5]
	D -> A [color=blue]
	J -> Y [color=blue]
	D -> T [color=blue]
	T -> K [color=blue]
	J -> K [color=blue]
	Z -> A [color=blue]
}
