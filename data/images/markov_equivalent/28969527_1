digraph {
	graph [rankdir=LR]
	U [height=.5 shape=plaintext width=.5]
	Y [height=.5 shape=plaintext width=.5]
	E [height=.5 shape=plaintext width=.5]
	F [height=.5 shape=plaintext width=.5]
	T [height=.5 shape=plaintext width=.5]
	N [height=.5 shape=plaintext width=.5]
	F -> E [color=blue]
	T -> N [color=blue]
	E -> Y [color=blue]
	U -> T [color=blue]
	F -> T [color=blue]
	F -> N [color=blue]
	E -> N [color=blue]
	E -> T [color=blue]
	Y -> T [color=blue]
	F -> Y [color=blue]
}
