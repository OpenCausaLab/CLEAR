digraph {
	graph [rankdir=LR]
	Y [height=.5 shape=plaintext width=.5]
	W [height=.5 shape=plaintext width=.5]
	X [height=.5 shape=plaintext width=.5]
	Z [height=.5 shape=plaintext width=.5]
	R [height=.5 shape=plaintext width=.5]
	Y -> Z [color=blue]
	X -> W [color=blue]
	Y -> W [color=blue]
	R -> Z [color=blue]
	X -> R [color=blue]
	W -> R [color=blue]
	Y -> X [color=blue]
	Y -> R [color=blue]
}
