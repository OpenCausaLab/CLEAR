digraph {
	graph [rankdir=LR]
	K [height=.5 shape=plaintext width=.5]
	P [height=.5 shape=plaintext width=.5]
	V [height=.5 shape=plaintext width=.5]
	T [height=.5 shape=plaintext width=.5]
	S [height=.5 shape=plaintext width=.5]
	P -> T [color=blue]
	K -> T [color=blue]
	V -> S [color=blue]
	P -> S [color=blue]
	T -> S [color=blue]
	K -> P [color=blue]
	K -> S [color=blue]
	P -> V [color=blue]
	K -> V [color=blue]
}
