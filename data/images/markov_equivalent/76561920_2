	U [height=.5 shape=plaintext width=.5]
	K [height=.5 shape=plaintext width=.5]
	V -> H [color=blue]
	U -> I [color=blue]
	I -> Z [color=blue]
	V -> Z [color=blue]
	I -> H [color=blue]
	U -> K [color=blue]
	I -> Q [color=blue]
}
