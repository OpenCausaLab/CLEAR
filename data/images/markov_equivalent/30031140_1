digraph {
	graph [rankdir=LR]
	B [height=.5 shape=plaintext width=.5]
	X [height=.5 shape=plaintext width=.5]
	K [height=.5 shape=plaintext width=.5]
	W [height=.5 shape=plaintext width=.5]
	J [height=.5 shape=plaintext width=.5]
	E [height=.5 shape=plaintext width=.5]
	L [height=.5 shape=plaintext width=.5]
	B -> E [color=blue]
	W -> L [color=blue]
	J -> L [color=blue]
	K -> L [color=blue]
	K -> B [color=blue]
	B -> J [color=blue]
	X -> J [color=blue]
}
