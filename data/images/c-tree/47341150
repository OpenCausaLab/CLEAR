digraph {
	graph [rankdir=LR]
	U [height=.5 shape=plaintext width=.5]
	K [height=.5 shape=plaintext width=.5]
	J [height=.5 shape=plaintext width=.5]
	O [height=.5 shape=plaintext width=.5]
	E [height=.5 shape=plaintext width=.5]
	K -> E [color=blue]
	U -> J [color=blue]
	U -> K [color=blue]
	U -> O [color=blue]
	K -> J [color=blue]
	O -> E [color=blue]
	J -> O [color=blue]
	U -> E [color=blue]
	K -> O [color=blue]
	J -> E [color=blue]
	K -> J [color=red dir=both]
}
