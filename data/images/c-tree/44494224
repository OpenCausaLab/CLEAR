digraph {
	graph [rankdir=LR]
	W [height=.5 shape=plaintext width=.5]
	C [height=.5 shape=plaintext width=.5]
	H [height=.5 shape=plaintext width=.5]
	F [height=.5 shape=plaintext width=.5]
	K [height=.5 shape=plaintext width=.5]
	W -> K [color=blue]
	C -> K [color=blue]
	H -> K [color=blue]
	F -> K [color=blue]
	H -> F [color=red dir=both]
	W -> K [color=red dir=both]
	H -> K [color=red dir=both]
	C -> F [color=red dir=both]
}
