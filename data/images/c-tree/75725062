digraph {
	graph [rankdir=LR]
	R [height=.5 shape=plaintext width=.5]
	Q [height=.5 shape=plaintext width=.5]
	Y [height=.5 shape=plaintext width=.5]
	S [height=.5 shape=plaintext width=.5]
	V [height=.5 shape=plaintext width=.5]
	Q -> S [color=blue]
	Q -> Y [color=blue]
	R -> V [color=blue]
	R -> S [color=blue]
	R -> Y [color=blue]
	Y -> S [color=blue]
	Y -> V [color=blue]
	Q -> S [color=red dir=both]
	Y -> V [color=red dir=both]
	R -> Q [color=red dir=both]
	R -> V [color=red dir=both]
}
