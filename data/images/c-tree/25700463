digraph {
	graph [rankdir=LR]
	S [height=.5 shape=plaintext width=.5]
	Y [height=.5 shape=plaintext width=.5]
	C [height=.5 shape=plaintext width=.5]
	A [height=.5 shape=plaintext width=.5]
	S -> A [color=blue]
	C -> A [color=blue]
	S -> Y [color=blue]
	Y -> C [color=blue]
	S -> C [color=blue]
	Y -> A [color=blue]
	S -> A [color=red dir=both]
	S -> Y [color=red dir=both]
	C -> A [color=red dir=both]
}
