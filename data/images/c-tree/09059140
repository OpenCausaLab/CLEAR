digraph {
	graph [rankdir=LR]
	A [height=.5 shape=plaintext width=.5]
	E [height=.5 shape=plaintext width=.5]
	W [height=.5 shape=plaintext width=.5]
	H [height=.5 shape=plaintext width=.5]
	E -> H [color=blue]
	A -> H [color=blue]
	W -> H [color=blue]
	E -> H [color=red dir=both]
	A -> H [color=red dir=both]
	W -> H [color=red dir=both]
}
