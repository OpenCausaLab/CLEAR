digraph {
	graph [rankdir=LR]
	E [height=.5 shape=plaintext width=.5]
	X [height=.5 shape=plaintext width=.5]
	P [height=.5 shape=plaintext width=.5]
	T [height=.5 shape=plaintext width=.5]
	S [height=.5 shape=plaintext width=.5]
	Y [height=.5 shape=plaintext width=.5]
	X -> S [color=blue]
	X -> Y [color=blue]
	T -> Y [color=blue]
	E -> Y [color=blue]
	P -> T [color=blue]
	P -> S [color=blue]
	X -> S [color=red dir=both]
	P -> Y [color=red dir=both]
	E -> T [color=red dir=both]
}
