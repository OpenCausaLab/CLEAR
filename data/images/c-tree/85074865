digraph {
	graph [rankdir=LR]
	W [height=.5 shape=plaintext width=.5]
	S [height=.5 shape=plaintext width=.5]
	V [height=.5 shape=plaintext width=.5]
	Y [height=.5 shape=plaintext width=.5]
	H [height=.5 shape=plaintext width=.5]
	W -> S [color=blue]
	S -> Y [color=blue]
	V -> Y [color=blue]
	W -> H [color=blue]
	S -> Y [color=red dir=both]
	W -> V [color=red dir=both]
	W -> Y [color=red dir=both]
	W -> H [color=red dir=both]
}
