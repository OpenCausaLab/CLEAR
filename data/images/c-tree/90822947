digraph {
	graph [rankdir=LR]
	C [height=.5 shape=plaintext width=.5]
	O [height=.5 shape=plaintext width=.5]
	J [height=.5 shape=plaintext width=.5]
	I [height=.5 shape=plaintext width=.5]
	M [height=.5 shape=plaintext width=.5]
	S [height=.5 shape=plaintext width=.5]
	O -> I [color=blue]
	M -> S [color=blue]
	J -> I [color=blue]
	C -> S [color=blue]
	C -> J [color=blue]
	O -> S [color=blue]
	O -> J [color=blue]
	C -> I [color=blue]
	O -> J [color=red dir=both]
}
