digraph {
	graph [rankdir=LR]
	A [height=.5 shape=plaintext width=.5]
	F [height=.5 shape=plaintext width=.5]
	I [height=.5 shape=plaintext width=.5]
	U [height=.5 shape=plaintext width=.5]
	M [height=.5 shape=plaintext width=.5]
	I -> U [color=blue]
	A -> M [color=blue]
	F -> I [color=blue]
	A -> F [color=blue]
	A -> F [color=red dir=both]
	I -> M [color=red dir=both]
	F -> U [color=red dir=both]
	F -> M [color=red dir=both]
}
