digraph {
	graph [rankdir=LR]
	E [height=.5 shape=plaintext width=.5]
	D [height=.5 shape=plaintext width=.5]
	S [height=.5 shape=plaintext width=.5]
	V [height=.5 shape=plaintext width=.5]
	E -> D [color=blue]
	E -> S [color=blue]
	E -> V [color=blue]
	S -> V [color=blue]
	D -> S [color=blue]
	S -> V [color=red dir=both]
}
