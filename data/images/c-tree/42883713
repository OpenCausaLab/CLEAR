digraph {
	graph [rankdir=LR]
	F [height=.5 shape=plaintext width=.5]
	Q [height=.5 shape=plaintext width=.5]
	U [height=.5 shape=plaintext width=.5]
	Y [height=.5 shape=plaintext width=.5]
	I [height=.5 shape=plaintext width=.5]
	Y -> I [color=blue]
	F -> Y [color=blue]
	U -> I [color=blue]
	Q -> Y [color=blue]
	U -> I [color=red dir=both]
	U -> Y [color=red dir=both]
	Q -> U [color=red dir=both]
	F -> U [color=red dir=both]
}
