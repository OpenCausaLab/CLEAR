digraph {
	graph [rankdir=LR]
	N [height=.5 shape=plaintext width=.5]
	J [height=.5 shape=plaintext width=.5]
	Q [height=.5 shape=plaintext width=.5]
	T [height=.5 shape=plaintext width=.5]
	E [height=.5 shape=plaintext width=.5]
	N -> Q [color=blue]
	T -> E [color=blue]
	Q -> E [color=blue]
	J -> T [color=blue]
	Q -> T [color=red dir=both]
	N -> J [color=red dir=both]
	J -> E [color=red dir=both]
	N -> T [color=red dir=both]
}
