digraph {
	graph [rankdir=LR]
	T [height=.5 shape=plaintext width=.5]
	W [height=.5 shape=plaintext width=.5]
	U [height=.5 shape=plaintext width=.5]
	K [height=.5 shape=plaintext width=.5]
	D [height=.5 shape=plaintext width=.5]
	T -> U [color=blue]
	W -> K [color=blue]
	K -> D [color=blue]
	U -> K [color=blue]
	T -> W [color=blue]
	W -> D [color=blue]
	W -> U [color=blue]
	T -> W [color=red dir=both]
	T -> D [color=red dir=both]
}
