	graph [rankdir=LR]
	J [height=.5 shape=plaintext width=.5]
	R [height=.5 shape=plaintext width=.5]
	G [height=.5 shape=plaintext width=.5]
	D [height=.5 shape=plaintext width=.5]
	B [height=.5 shape=plaintext width=.5]
	U [height=.5 shape=plaintext width=.5]
	O [height=.5 shape=plaintext width=.5]
	D -> U [color=blue]
	R -> O [color=blue]
	R -> D [color=blue]
	R -> B [color=blue]
	J -> B [color=blue]
	G -> O [color=blue]
	U -> O [color=blue]
}
