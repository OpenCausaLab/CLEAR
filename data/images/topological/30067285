digraph {
	graph [rankdir=LR]
	Q [height=.5 shape=plaintext width=.5]
	R [height=.5 shape=plaintext width=.5]
	U [height=.5 shape=plaintext width=.5]
	Z [height=.5 shape=plaintext width=.5]
	O [height=.5 shape=plaintext width=.5]
	Q -> R [color=blue]
	U -> O [color=blue]
	U -> Z [color=blue]
	R -> Z [color=blue]
	Q -> U [color=blue]
	Z -> O [color=blue]
	R -> U [color=blue]
	R -> O [color=blue]
	Q -> Z [color=blue]
	Q -> O [color=blue]
	Q -> Z [color=red dir=both]
}
