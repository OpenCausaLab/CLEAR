digraph {
	graph [rankdir=LR]
	H [height=.5 shape=plaintext width=.5]
	D [height=.5 shape=plaintext width=.5]
	F [height=.5 shape=plaintext width=.5]
	K [height=.5 shape=plaintext width=.5]
	Q [height=.5 shape=plaintext width=.5]
	U [height=.5 shape=plaintext width=.5]
	H -> U [color=blue]
	D -> F [color=blue]
	H -> D [color=blue]
	H -> K [color=blue]
	Q -> U [color=blue]
	H -> Q [color=blue]
	D -> U [color=blue]
	K -> U [color=blue]
	F -> Q [color=red dir=both]
	D -> Q [color=red dir=both]
}
