digraph {
	graph [rankdir=LR]
	F [height=.5 shape=plaintext width=.5]
	O [height=.5 shape=plaintext width=.5]
	L [height=.5 shape=plaintext width=.5]
	H [height=.5 shape=plaintext width=.5]
	M [height=.5 shape=plaintext width=.5]
	D [height=.5 shape=plaintext width=.5]
	F -> L [color=blue]
	L -> H [color=blue]
	L -> M [color=blue]
	F -> O [color=blue]
	O -> D [color=blue]
	M -> D [color=blue]
	O -> L [color=blue]
	F -> D [color=blue]
	O -> L [color=red dir=both]
}
