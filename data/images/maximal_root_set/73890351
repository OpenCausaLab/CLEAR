digraph {
	graph [rankdir=LR]
	Z [height=.5 shape=plaintext width=.5]
	I [height=.5 shape=plaintext width=.5]
	W [height=.5 shape=plaintext width=.5]
	S [height=.5 shape=plaintext width=.5]
	R [height=.5 shape=plaintext width=.5]
	Z -> W [color=blue]
	Z -> S [color=blue]
	Z -> R [color=blue]
	Z -> I [color=blue]
	W -> S [color=blue]
	I -> W [color=blue]
	I -> S [color=blue]
	I -> R [color=red dir=both]
	Z -> W [color=red dir=both]
	S -> R [color=red dir=both]
	Z -> R [color=red dir=both]
}
