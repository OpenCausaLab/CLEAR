digraph {
	graph [rankdir=LR]
	Z [height=.5 shape=plaintext width=.5]
	C [height=.5 shape=plaintext width=.5]
	W [height=.5 shape=plaintext width=.5]
	L [height=.5 shape=plaintext width=.5]
	C -> W [color=blue]
	Z -> W [color=blue]
	W -> L [color=blue]
	Z -> C [color=red dir=both]
	W -> L [color=red dir=both]
	C -> L [color=red dir=both]
}
