digraph {
	graph [rankdir=LR]
	I [height=.5 shape=plaintext width=.5]
	V [height=.5 shape=plaintext width=.5]
	T [height=.5 shape=plaintext width=.5]
	Q [height=.5 shape=plaintext width=.5]
	M [height=.5 shape=plaintext width=.5]
	N [height=.5 shape=plaintext width=.5]
	T -> I [color=blue]
	V -> N [color=blue]
	V -> Q [color=blue]
	M -> T [color=blue]
	N -> M [color=blue]
	I -> Q [color=blue]
}
